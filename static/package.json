    "react-infinite-scroller": "^1.2.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.16.2",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "reselect-tools": "^0.0.7",
    "sass": "^1.14.3",
    "webpack": "^4.27.1"
  },
  "standard": {

{
  "name": "@fire-marshal/static",
  "version": "0.5.0",
  "description": "static project of fire-marshal",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production cd public && parcel --out-dir ../build build index.html",
    "lint": "npm run lint:src && npm run lint:tests",
    "lint:src": "standard 'src/**/*.js'",
    "lint:tests": "standard --env mocha 'tests**/*.js'",
    "start": "NODE_ENV=development cd public && parcel --hmr-hostname 0.0.0.0 --hmr-port 7001 --out-dir ../build --cache-dir ../.cache watch index.html",
    "test": "mocha-webpack --opts ./tests/mocha.opts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fire-marshal/fire-marshal.git"
  },
  "keywords": [
    "fire",
    "wildfire",
    "nasa",
    "space-app-challenge",
    "hackathon"
  ],
  "author": {
    "name": "Eugene Krenevenets",
    "email": "ievgenii.krevenets@gmail.com",
    "url": "http://hyzhak.github.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fire-marshal/fire-marshal/issues"
  },
  "homepage": "https://github.com/fire-marshal/fire-marshal#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-syntax-export-default-from": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.2.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "mocha": "^5.2.0",
    "mocha-webpack": "2.0.0-beta.0",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.2.0",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "connected-react-router": "^4.5.0",
    "decko": "^1.2.0",
    "eventemitter3": "^3.1.0",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "lodash": "^4.17.11",
    "parcel-bundler": "^1.10.3",
    "prop-types": "^15.6.2",
